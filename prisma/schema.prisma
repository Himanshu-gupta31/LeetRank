generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id            String @id @default(cuid())
  clerkId       String @unique
  username      String? @unique 
  email         String? @unique 
  clerkusername String? @unique 
  college       String?

  Ranking Ranking[]
}

model Ranking {
  id         String @id @default(cuid())
  userId     String
  totalques  String
  hardques   String
  mediumques String
  easyques   String
  user       User   @relation(fields: [userId], references: [id])

  @@unique([userId, id])
}
